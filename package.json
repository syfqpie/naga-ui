{
	"name": "naga-ui",
	"description": "Webcomponent naga-ui following open-wc recommendations",
	"license": "MIT",
	"author": "naga-ui",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "dist/src/index.js",
	"module": "dist/src/index.js",
	"exports": {
		".": "./dist/src/index.js",
		"./naga-ui.js": "./dist/src/naga-ui.js"
	},
	"scripts": {
		"analyze": "cem analyze --litelement",
		"start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
		"build": "tsc && npm run analyze -- --exclude dist",
		"prepublish": "tsc && npm run analyze -- --exclude dist",
		"test": "tsc && wtr --coverage --node-resolve --playwright --browsers chromium firefox webkit",
		"test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch --coverage --node-resolve --playwright --browsers chromium firefox webkit\"",
		"storybook": "tsc && npm run analyze -- --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
		"storybook:build": "tsc && npm run analyze -- --exclude dist && build-storybook"
	},
	"dependencies": {
		"lit": "^2.0.2"
	},
	"devDependencies": {
		"@custom-elements-manifest/analyzer": "^0.4.17",
		"@open-wc/testing": "^3.1.6",
		"@web/dev-server": "^0.1.34",
		"@web/dev-server-storybook": "^0.5.4",
		"@web/test-runner": "^0.14.0",
		"@web/test-runner-playwright": "^0.11.0",
		"concurrently": "^5.3.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.2"
	},
	"customElements": "custom-elements.json"
}
